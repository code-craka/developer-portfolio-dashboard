{
  "name": "developer-portfolio-dashboard",
  "version": "0.1.0",
  "private": false,
  "license": "MIT",
  "description": "A modern, full-stack developer portfolio website with an integrated admin dashboard for content management",
  "keywords": ["portfolio", "dashboard", "nextjs", "typescript", "tailwindcss", "clerk", "neondb"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/developer-portfolio-dashboard.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/developer-portfolio-dashboard/issues"
  },
  "homepage": "https://github.com/yourusername/developer-portfolio-dashboard#readme",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:unit": "vitest --run test/lib test/components",
    "test:integration": "vitest --run test/api",
    "test:e2e": "vitest --run test/e2e",
    "init-db": "tsx scripts/init-db.ts",
    "reset-db": "tsx -e \"import { DatabaseMigrations } from './lib/migrations'; DatabaseMigrations.resetDatabase();\"",
    "test-auth": "tsx scripts/test-auth-setup.ts",
    "test-projects": "tsx scripts/test-projects-api.ts",
    "test-projects-http": "tsx scripts/test-projects-http.ts",
    "test-projects-complete": "tsx scripts/test-projects-complete.ts",
    "test-experiences": "tsx scripts/test-experiences-api.ts",
    "test-experiences-http": "tsx scripts/test-experiences-http.ts",
    "verify-experiences": "tsx scripts/verify-experiences-api.ts",
    "test-contact": "tsx scripts/test-contact-api.ts",
    "test-contact-http": "tsx scripts/test-contact-http.ts",
    "test-hero": "tsx scripts/test-hero-section.ts"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.31.1",
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@neondatabase/serverless": "^0.9.0",
    "clsx": "^2.0.0",
    "dotenv": "^17.2.1",
    "framer-motion": "^11.18.2",
    "next": "^15.4.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0",
    "svix": "^1.73.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.3.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.17",
    "eslint": "^9.33.0",
    "eslint-config-next": "15.4.6",
    "jsdom": "^26.1.0",
    "msw": "^2.10.5",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "tsx": "^4.7.0",
    "typescript": "^5.0.0",
    "vitest": "^3.2.4"
  }
}
